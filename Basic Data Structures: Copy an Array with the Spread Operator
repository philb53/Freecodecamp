function copyMachine(arr, num) {
  let newArr = [];
  while (num >= 1) {
    // change code below this line
newArr.unshift([...arr]);
    // change code above this line
    num--;
  }
  return newArr;
}

// change code here to test different cases:
console.log(copyMachine([true, false, true], 2));

//newArr = where copy array is located
// .unshift() to add to newArr from arr
// [...arr] spread operator copies contents of arr
